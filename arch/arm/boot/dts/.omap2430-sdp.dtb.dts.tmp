/ {
 model = "TI OMAP2430 SDP";
 compatible = "ti,omap2430-sdp", "ti,omap2430", "ti,omap2";

 memory {
  device_type = "memory";
  reg = <0x80000000 0x8000000>;
 };
};

&i2c2 {
 clock-frequency = <100000>;

 twl: twl@48 {
  reg = <0x48>;
  interrupts = <7>;
 };
};

