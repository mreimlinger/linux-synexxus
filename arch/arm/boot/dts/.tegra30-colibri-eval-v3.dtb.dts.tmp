  clock-frequency = <100000>;
 };





 i2c@7000d000 {
  status = "okay";
  clock-frequency = <100000>;

  pmic: tps65911@2d {
   compatible = "ti,tps65911";
   reg = <0x2d>;

   interrupts = <0 86 4>;
