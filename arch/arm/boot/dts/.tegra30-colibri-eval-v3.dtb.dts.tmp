  status = "okay";
  clock-frequency = <100000>;


  rtc@68 {
   compatible = "stm,m41t00";
   reg = <0x68>;
  };
 };


 hdmiddc: i2c@7000c700 {
  status = "okay";
 };


 spi@7000d400 {
  status = "okay";
  spi-max-frequency = <25000000>;
  can0: can@0 {
   compatible = "microchip,mcp2515";
   reg = <0>;
