    nvidia,tristate = <0>;
   };


   gmi_a16_pj7 {
    nvidia,pins = "gmi_a16_pj7",
      "gmi_a17_pb0",
      "gmi_a18_pb1",
      "gmi_a19_pk7";
    nvidia,function = "uartd";
    nvidia,pull = <0>;
    nvidia,tristate = <0>;
   };


   uart2_rxd {
