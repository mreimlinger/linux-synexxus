   interrupt-parent = <&gpio>;
   interrupts = <((18 * 8) + 0) 1>;
   spi-max-frequency = <10000000>;
  };
  spidev0: spi@1 {
   compatible = "spidev";
   reg = <1>;
   spi-max-frequency = <25000000>;
  };
 };

 sdhci@78000200 {
  status = "okay";
  bus-width = <4>;
  cd-gpios = <&gpio ((2 * 8) + 7) 1>;
