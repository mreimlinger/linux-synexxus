  rtc0 = "/i2c@7000c000/rtc@68";
  rtc1 = "/i2c@7000d000/tps65911@2d";
  rtc2 = "/rtc@7000e000";
 };

 host1x@50000000 {
  dc@54200000 {
   rgb {
    status = "okay";
    nvidia,panel = <&panel>;
   };
  };
  hdmi@54280000 {
   status = "okay";
  };
 };

 serial@70006000 {
  status = "okay";
