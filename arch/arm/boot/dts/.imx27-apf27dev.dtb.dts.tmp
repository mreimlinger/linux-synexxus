 imx27-apf27dev {
  pinctrl_cspi1: cspi1grp {
   fsl,pins = <
    0x7e 0x004 0x0
    0x7f 0x004 0x0
    0x7d 0x004 0x0
   >;
  };

  pinctrl_cspi1_cs: cspi1csgrp {
   fsl,pins = <0x7c 0x032 0x0>;
  };

  pinctrl_cspi2: cspi2grp {
   fsl,pins = <
